{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "startCommand": "java -jar -Dspring.profiles.active=prod app.jar",
    "healthcheckPath": "/actuator/health",
    "healthcheckTimeout": 300
  }
}

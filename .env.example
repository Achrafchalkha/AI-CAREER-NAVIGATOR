# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control

# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority&ssl=true&authSource=admin&appName=YourApp
MONGODB_DATABASE=aicareernavigator

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here_minimum_256_bits
JWT_EXPIRATION=86400000

# OpenRouter API Configuration
OPENROUTER_API_KEY=sk-or-v1-your_openrouter_api_key_here

# Optional: Override default configurations
# SERVER_PORT=8036
# CORS_ALLOWED_ORIGINS=http://localhost:4200
